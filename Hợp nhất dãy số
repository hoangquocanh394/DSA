#include <bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin >> t;
	while(t--){
		long long n,m;
	    cin >> n >> m;
	    long long a[n],b[m];
		vector<long long> s;
	    for(long long i=0;i<n;i++){
	    	cin >> a[i];
	    	s.push_back(a[i]);
		}
		for(long long i=0;i<m;i++){
	    	cin >> b[i];
	    	s.push_back(b[i]);
		}
		sort(s.begin(),s.end());
		for(auto x:s){
			cout << x << " ";
		}
		cout << endl;
	}
    return 0;
}


